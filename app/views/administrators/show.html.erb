<p>
<strong>Title:</strong>
<%=	@administrator.title %>
</p>
<p>
<strong>Author:</strong>
<%=	@administrator.author %>
</p>
<p>
<strong>ISBN:</strong>
<%=	@administrator.isbn %>
</p>
<p>
<strong>Copies:</strong>
<%=	@administrator.copies %>
</p>
<h1> View Reviews </h1>
<%	@administrator.patrons.each do	|patron|	%>
    <p>
    <strong>Name:</strong>
    <%=	patron.name %>
    </p>
    <p>
    <strong>Review:</strong>
    <%=	patron.review %>
    </p>
    <%=	link_to 'Destroy Review',	 [patron.administrator,	patron],
    method:	:delete,
    data:	{	confirm:	'Are	you	 sure?'	}	%>
<%	end	%>
</br>
<%=	link_to 'Back',	administrator_patrons_path(1) %>