<h1>Administrators</h1>
<%=	form_with(model: @administrator,	local:	true)	 do	|form|	%>
    <%	if @administrator.errors.any?	%>
    <div	id="error_explanation">
    <h2>
    error/s	prohibited
    this administrator from being saved:
    </h2>
    <ul>
    <%	@administrator.errors.full_messages.each do	|msg|	%>
    <li><%=	msg %></li>
    <%	end	%>
    </ul>
    </div>
    <%	end	%>
    <p>
    <%=	form.label :title %><br>
    <%=	form.text_field:title %>
    </p>
    <p>
    <%=	form.label :author %><br>
    <%=	form.text_field:author %>
    </p>
    <p>
    <%=	form.label :isbn %><br>
    <%=	form.text_field:isbn %>
    </p>
    <p>
    <%=	form.label :copies %><br>
    <%=	form.text_field:copies %>
    </p>
    <p>
    <%=	form.submit %>
    </p>
 <%	end	%>
 <%=link_to 'Back',	administrators_path %>